	<subDataset name="cirugias" uuid="41de8b73-7858-4ee6-86b7-fca6671146b3">
		<parameter name="fechaInicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="fechaFin" class="java.util.Date"/>
		<parameter name="idPaciente" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
    informix.notaquirposopera.fechaelaboracion,
    informix.servicio.descservicio,
    informix.usuario.nombre,
    informix.cie09.descdiag,
    informix.operanotaquirposopera.principal,
    informix.seguimiento_hosp_cirugia.costo
FROM
    informix.notaquirposopera
INNER JOIN
    informix.operanotaquirposopera
ON
    (
        informix.notaquirposopera.idnota = informix.operanotaquirposopera.idnota)
INNER JOIN
    informix.seguimiento_hosp_cirugia
ON
    (
        informix.operanotaquirposopera.idnota = informix.seguimiento_hosp_cirugia.idnota)
AND (
        informix.operanotaquirposopera.iddiagnostico =
        informix.seguimiento_hosp_cirugia.iddiagnostico)
AND (
        informix.operanotaquirposopera.tipodiag = informix.seguimiento_hosp_cirugia.tipodiag)
INNER JOIN
    informix.cie09
ON
    (
        informix.seguimiento_hosp_cirugia.iddiagnostico = informix.cie09.iddiagnostico)
INNER JOIN
    informix.servicio
ON
    (
        informix.notaquirposopera.idservicio = informix.servicio.idservicio)
INNER JOIN
    informix.usuario
ON
    (
        informix.notaquirposopera.idusuario = informix.usuario.idusuario)
WHERE notaquirposopera.fechaelaboracion between $P{fechaInicio} and $P{fechaFin}
and notaquirposopera.idpaciente  = $P{idPaciente}
ORDER BY notaquirposopera.fechaelaboracion,principal]]>
		</queryString>
		<field name="fechaelaboracion" class="java.sql.Date"/>
		<field name="descservicio" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="descdiag" class="java.lang.String"/>
		<field name="principal" class="java.lang.String"/>
		<field name="costo" class="java.math.BigDecimal"/>
		<variable name="importeTotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{costo}]]></variableExpression>
		</variable>
		<variable name="tipo" class="java.lang.String">
			<variableExpression><![CDATA[$F{principal} == 's'?'SECUNDARIO':'PRINCIPAL']]></variableExpression>
		</variable>
	</subDataset>
