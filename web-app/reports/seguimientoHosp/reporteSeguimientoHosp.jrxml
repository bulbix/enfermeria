	<subDataset name="medicamentos" uuid="1fe7ba0d-388d-4210-a26f-023d84a9e9ab">
		<parameter name="fechaInicio" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="fechaFin" class="java.util.Date"/>
		<parameter name="idPaciente" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
    informix.articulo.cve_art,
    informix.articulo.des_articulo,
    informix.articulo.unidad,
    informix.seguimiento_hosp_medicamento.precio_unitario,
    sum(informix.seguimiento_hosp_medicamento.cantidad) as cantidad
FROM
    informix.seguimiento_hosp_medicamento
INNER JOIN
    informix.articulo
ON
    (
        informix.seguimiento_hosp_medicamento.cve_art = informix.articulo.cve_art)
INNER JOIN
    informix.seguimiento_hosp
ON
    (
        informix.seguimiento_hosp_medicamento.idseguimiento = informix.seguimiento_hosp.id)
WHERE fecha_elaboracion between $P{fechaInicio} and $P{fechaFin}
and seguimiento_hosp.idpaciente  = $P{idPaciente}
group BY 1,2,3,4
order by 1]]>
		</queryString>
		<field name="cve_art" class="java.lang.Integer"/>
		<field name="des_articulo" class="java.lang.String"/>
		<field name="unidad" class="java.lang.String"/>
		<field name="precio_unitario" class="java.math.BigDecimal"/>
		<field name="cantidad" class="java.math.BigDecimal"/>
		<variable name="importe" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{cantidad} * $F{precio_unitario}]]></variableExpression>
		</variable>
		<variable name="importeTotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{importe}]]></variableExpression>
		</variable>
		<group name="group1">
			<groupExpression><![CDATA[$F{cve_art}]]></groupExpression>
		</group>
	</subDataset>
